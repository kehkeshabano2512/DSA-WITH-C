//linear search
#include <iostream>
using namespace std;
int main()
{
	int num[10],i,key;
	
	cout<<"enter 10  Array elements:"<<"\n";
	for(int i=0;i<10;i++){
		cin>>num[i];
	}
	cout<<"enter the item you want to search";
	cin>>key;
	for(i=0;i<10;i++)
	{
		if(key==num[i])
		{
			cout<<"search successfull"<<"index :"<<i;
			break;
		}
		//else block
	}
	if(i==10)
	{
		cout<<"search unsuccessfull"<<i;
	}
	return 0;
	}
